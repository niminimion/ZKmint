{
  "version": 2,
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/server.js" },
    { "source": "/callback", "destination": "/api/server.js" },
    { "source": "/", "destination": "/api/server.js" },
    { "source": "/(.*)", "destination": "/api/server.js" }
  ]
}


